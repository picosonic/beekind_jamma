{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cmake-build Debug",
			"type": "shell",
			"command": "cmake --build . -- -j10",
			"options": {
				"cwd": "${workspaceRoot}/build.debug"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$gcc"]
		}
	]
}